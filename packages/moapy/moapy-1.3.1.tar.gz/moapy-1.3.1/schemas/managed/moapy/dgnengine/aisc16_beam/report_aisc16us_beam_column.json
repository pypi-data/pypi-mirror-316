          },
          "Mx": {
            "title": "Moment about X-axis",
            "description": "Bending moment acting about the member's major axis (X-axis). This represents the primary flexural action in the strong axis direction.",
            "$ref": "#/components/schemas/Moment"
          },
          "My": {
            "title": "Moment about Y-axis",
            "description": "Bending moment acting about the member's minor axis (Y-axis). This represents the secondary flexural action in the weak axis direction.",
