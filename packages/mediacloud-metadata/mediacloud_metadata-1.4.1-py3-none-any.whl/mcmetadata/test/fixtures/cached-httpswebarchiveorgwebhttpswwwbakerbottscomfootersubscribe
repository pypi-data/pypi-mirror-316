<script src="/web/20230930025619js_/https://www.bakerbotts.com/assets/src/js/iealert/iealert.min.js"></script>
<script>
    $(document).ready(function () {
        var hasCookie = Cookies.get('bakerbotts-cookie');
        if (hasCookie)
            $("#sliding-popup").hide();
        else
            $("#sliding-popup").addClass('is-animating');

        $(".agree-button").on("click", function () {
            Cookies.set("bakerbotts-cookie", 1, { expires: 1000, path: "/" });
            $("#sliding-popup").fadeOut();
        });

        var ieAlertCookie = Cookies.get('bakerbotts-iealert');
        if (!ieAlertCookie) {
            $("body").iealert({
                support: "ie8",
                title: $(".js-browsernotice-title").val(),
                text: $(".js-browsernotice-text").val(),
                upgradeTitle: $(".js-browsernotice-upgradetitle").val(),
                upgradeLink: $(".js-browsernotice-upgradelink").val()
            });
            Cookies.set("bakerbotts-iealert", 1, { expires: 1000, path: "/" });
        }
    });

</script>
