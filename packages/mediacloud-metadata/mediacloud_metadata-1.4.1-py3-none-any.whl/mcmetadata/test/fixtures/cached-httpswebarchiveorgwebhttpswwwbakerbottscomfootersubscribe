<section data-global-search=".global-search-btn" class="global-search">
<div class="content-wrapper content-wrap">
<div class="global-search-box pull-left">
<script>
                $(document).ready(function () {
                    $('input.globalsearch').keyup(function (e) {
                        if (e.keyCode == '13') {
                            $('.globalsearchbtn').click();
                        } else if ($('input.globalsearch').val().length > 2) {
                            $.ajax({
                                type: "GET",
                                url: '/api/sitecore/Components/LoadSuggestedResults' + '?query=' + $('input.globalsearch').val(),
                                crossDomain: false,
                                dataType: "text"
                            }).done(function (data) {
                                $('.suggested-results').html(data).show();
                                $('.suggested-results a').each(function () {
                                    var term = $('input.globalsearch').val();
                                    var keywords = new RegExp("(" + term + ")", "gi");
                                    $(this).html($(this).text().replace(keywords, '<i>$1</i>'));
                                });

                                var left = $('input.globalsearch').offset().left;
                                var top = $('.global-search-box').position().top + $('.global-search-box').outerHeight(true);
                                var width = $('.global-search-box').width();
                                $('.suggested-results').css({ 'left': left, 'width': width, 'max-width': width, 'top': top + 3 });

                            });
                        } else if ($('input.globalsearch').val().length <= 2) {
                            $('.suggested-results').html('');
                        }
                    });

                    $('.globalsearchbtn').click(function () {
                        globalSearch($('input.globalsearch').val());
                    });

                    $('input.globalsearch').keydown(function (e) {
                        if (e.keyCode == '13') {
                            $('.globalsearchbtn').click();
                        }
                    });

                    $('.clearall').click(function () {
                        // To be done...

                    });
                    $(document).on('click', '.suggested-results a', function () {

                        var currentSearch = $(this).text();
                        $('input.globalsearch').val(currentSearch);
                        $('.suggested-results').hide();
                        globalSearch();
                    });



                });


                function globalSearch() {


                    var passval = $('input.globalsearch').val().trim();
                    var gelastchr = passval.substring(passval.length - 1);
                    if (gelastchr == "'") {

                        passval = passval.substring(0,passval.length - 1);
                    }
                    else if(gelastchr == "\"")
                    {
                        var geSecondlastchr = passval.substring(passval.length - 2);
                        if (geSecondlastchr == "'\"") {

                            passval = passval.substring(0, passval.length - 2);
                            passval = passval + "\"";
                        }
                    }


                    while (passval.indexOf("&") > 0) {

                        passval = passval.replace("&", "$=");

                    }
                    while (passval.indexOf("'") != -1) {

                        passval = passval.replace("'", "$*");

                    }


                    while (passval.indexOf("\"") != -1) {

                        passval = passval.replace("\"", "$!");

                    }



                    window.location.href = '/sitesearch' + '?query=' + passval + '&skip=' + '0' + '&take=' + '10';




                }
                $(window).on("orientationchange", function () {
                    if ($('.suggested-results').is(':visible')) {
                        $('.suggested-results').html('').hide();

                    }
                });

                /* below code added by YOGESH for over ride hover effect on featureSlide class by featureSlideAvoidHover , which invocked on .NET condition */
                $(window).on(".featurePods .podsBelt .featureSlideAvoidHover").hover(
                    function () {

                        $(this).css({ "color": "#a7a7a7", "background": "transparent" });
                        $(".featurePods .podsBelt .featureSlideAvoidHover ul li a").css({ "color": "#379f9b", "background": "transparent" });
                    }
                  );
            </script>
<input class="globalsearch" type="text" placeholder="Search">
<button class="globalsearchbtn"><i class="fa fa-search"></i></button>
</div>
