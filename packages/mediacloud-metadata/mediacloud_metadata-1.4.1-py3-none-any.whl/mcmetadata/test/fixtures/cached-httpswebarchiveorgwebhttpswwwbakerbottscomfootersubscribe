<script>
    var addToHomescreen,
        hascookiePtabService;


    if (typeof (window.jQuery) !== "undefined") {
        $(document).ready(function () {
            addToHomescreen = {
                autoStart: true,
                displayPace: 1440, // minutes -> once per day
                startDelay: 3, //seconds
                lifespan: 5, //seconds
                skipFirstVisit: false,
                maxDisplayCount: 0, // maximum number of times the callout will be displayed
                icon: true
            };

            $.getScript("/assets/src/js/libs/addtohomescreen.min.js", function () {
                hascookiePtabService = Cookies.get('bakerbotts-ptabservice');

                if (!hascookiePtabService) {
                    //always show the popup for the ptab-trials service
                    if (window.location.href.indexOf('ptab-trials') !== -1) {
                        Cookies.set("bakerbotts-ptabservice", 1, { expires: 1440 });
                        addToHomescreen.removeSession();
                    }
                }

                // call the addToHomescreen script to initialize
                //addToHomescreen();

            }, true);
        });
    }
</script>
