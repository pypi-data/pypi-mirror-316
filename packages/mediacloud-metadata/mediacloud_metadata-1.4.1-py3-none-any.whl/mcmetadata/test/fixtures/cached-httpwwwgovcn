			//繁体和简体相互转换
			var currUrl = window.location.href;

			var flag = currUrl.indexOf("big5.www.gov.cn/gate/big5/");

			var jtzw = 'ht'+'tps://www.gov.cn';

			//繁体
			if (flag > 0) {

				$('.header_logo a').attr('href',jtzw);

				$('.header_toolbar li').eq(2).find('a').attr('href', jtzw);

			}//简体
			else {

				$('.header_toolbar li').eq(2).find('a').attr('href', jtzw);

			};
		</script>
