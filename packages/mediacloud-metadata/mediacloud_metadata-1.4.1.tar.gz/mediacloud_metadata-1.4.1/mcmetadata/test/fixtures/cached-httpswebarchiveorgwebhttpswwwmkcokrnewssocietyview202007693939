<script type="text/javascript">
$().ready(function() {
    var maxt = 0, rt = 3.45, weight = 0.4, st;
    var intv = setInterval(function () {
        var cont = $('.art_txt');
        try {
            var wc = cont.html().replace(/(<([^>]+)>)/gi, "").replace('window.jQuery || document.write("")', '').split(/\s+/).length;
            if(wc > 0) {
                maxt = (wc/rt)*weight;
                st = new Date();
                clearInterval(intv);
            }
        } catch(err) {}
    }, 500);


    $(window).on('beforeunload ',function() {
        //ga('send', 'event', 'rating', 10);
        var diff = ((new Date()).getTime() - st.getTime())/1000, rating = (diff/maxt)*10;
        if(rating > 10) rating = 10;

        //console.log(diff + ' | ' + maxt);
        //console.log(parseInt(rating));
    });
});
</script>


