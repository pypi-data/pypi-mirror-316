    var pageReated = function (idx) {
        if(idx > rel_limit) idx = 0;
        else if(idx <0) idx = rel_limit;

        rel_idx = idx;

        var list_html = '';
        try {
            for (var i = 0; i < Object.keys(rel_json[idx]).length; i++) {
                list_html += '<li> <a href="' + rel_json[idx][i]['link'] + '" data-category="related" data-label="' + rel_json[idx][i]['year'] + '/' + rel_json[idx][i]['no'] + '" ga-evnt-ctg="page:뉴스-기사"  ga-act-ctg="page:section:관련뉴스"  ga-evnt-idx="' + (i + 1 < 10 ? '0': '') + (i + 1) + '-' + rel_json[idx][i]['year'] + '' + rel_json[idx][i]['no'] + '" >' + rel_json[idx][i]['title'] + '</a></li>';
            }
        } catch(e) {}
        $('.rel_art').html(list_html);
    };
    </script>
