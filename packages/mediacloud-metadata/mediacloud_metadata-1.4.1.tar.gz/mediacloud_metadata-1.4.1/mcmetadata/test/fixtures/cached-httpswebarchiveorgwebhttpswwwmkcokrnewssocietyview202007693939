<script type="text/javascript">
    $().ready(function () {
        var tg = $('#bestclick div').not('.tab_bestclick').not('.many_art_tit').not('#google_dfp_BR_300x100,300x125,fluid'), mn = $('.tab_bestclick li'), mn_btn = mn.find('a'), idx = 0; //
        var tabname = {
            '종합': 0, '연예': 1, '스포츠': 2
        };
        try {
            if (typeof right_tab1 != 'undefined' && right_tab1 != '') idx = tabname[right_tab1];
        } catch (e) {}

        function bestclickShow(n) {
            console.log('n : '+n);
            mn.removeClass('on').eq(n).addClass('on');
            tg.hide().eq(n).show();
        }

        mn_btn.on('click', function (e) {
            bestclickShow(mn_btn.index($(this)));
            e.preventDefault();
        });

        bestclickShow(idx);
    });
</script>
