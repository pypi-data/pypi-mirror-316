// Paywall Bounce Rate pixel
function sendCustomPixel(method, consent, tcfStatus) {
    var pixelUrl = "https://web.archive.org/web/20220721144454/https://bpt.webedia-group.com/pixel.png?/domain=" + window.location.hostname + "&referrer=" + document.referrer +  "&site_route=" + jadConfig.page + "&event=tcfapi&method=" + method + "&consent=" + consent + "&tcfStatus=" + tcfStatus
    var img = document.createElement('img');
    img.style = "display: none;"
    img.src = pixelUrl;
    document.body.appendChild(img);
}

function triggerOnReadyOrTimeout(action, tcfStatus) {

    var done = false;

    function doItIfNotDone(method, consent, tcfStatus) {
        if (!done) {
            action(method, consent, tcfStatus);
            done = true;
        }
    }

    window.didomiOnReady = window.didomiOnReady || [];
    window.didomiOnReady.push(function (Didomi) {
        var consent = Didomi.isConsentRequired() ? Didomi.getUserConsentStatusForVendor("c:googleana-XMqagawa") && Didomi.getUserConsentStatusForVendor("google") : true;
        var consentToString = consent == undefined ? "undefined" : consent;
        doItIfNotDone("didomiOnReady", consentToString, tcfStatus);
    });

    setTimeout(function() {doItIfNotDone("timeout", "false", tcfStatus)}, 1000);
}

var tcfok = false;

var tryTcfApiTimer = 0;

var tryToReachTcfApi = setInterval(function() {
    if (typeof __tcfapi !== "undefined") {
        clearInterval(tryToReachTcfApi);
        launchTcfApi()
    } else if (tryTcfApiTimer > 3000) {
        clearInterval(tryToReachTcfApi);
        triggerOnReadyOrTimeout(sendCustomPixel, "tcfnotloaded")
    }
    tryTcfApiTimer += 200;
}, 200)

function launchTcfApi() {
    __tcfapi('addEventListener', 2, function(tcData, success) {
        if (success) {
            try{
                switch (tcData.eventStatus) {
                    case "useractioncomplete":
                        triggerOnReadyOrTimeout(sendCustomPixel, tcData.eventStatus);
                        __tcfapi('removeEventListener', 2, function(){}, tcData.listenerId)
                        break;
                    case "cmpuishown":
                        triggerOnReadyOrTimeout(sendCustomPixel, tcData.eventStatus);
                        break;
                    case "tcloaded":
                        triggerOnReadyOrTimeout(sendCustomPixel, tcData.eventStatus);
                        __tcfapi('removeEventListener', 2, function(){}, tcData.listenerId)
                        break;
                    default:
                        triggerOnReadyOrTimeout(sendCustomPixel, tcData.eventStatus);
                }
            } catch (error) {
                triggerOnReadyOrTimeout(sendCustomPixel, tcData.eventStatus);
            }
            tcfok = true;
        }
    });

    setTimeout(function() {
        if (!tcfok) {
            triggerOnReadyOrTimeout(sendCustomPixel, "tcfnotloaded")
        }
    }, 2000);
}

// Adding nbpageview
var nbpageviews = sessionStorage.getItem('nbpageviews');
jadConfig.targeting.page = nbpageviews;

jad.cmd.push(function() {
    jad.public.setConfig(jadConfig);
    jad.public.loadPositions();
    jad.public.displayPositions();

    jad.public.addEventListener("positionRenderEnded", function(event) {

        var divId = event.slot.getSlotElementId();

        if (["header", "rectangle_atf", "rectangle_mtf", "rectangle_btf", "inread1", "inread2", "footer"].includes(divId)) {
            var element = document.getElementById(divId);
            if(!event.isEmpty){
                element.parentElement.classList.add("adActive");
            } else {
                element.parentElement.classList.remove("adActive");
            }
        }
    })

});

    var wadsDiapoIsloaded = false;
    var wadsNextAdIsloaded = false;

    var pageArticleDiapoPositions = [];

    var jadConfigDiapo = Object.assign({}, jadConfig, {
        page: "/120157152/PUREMEDIAS_FR_WEB/diaporama",
        autoRefreshEnabled: false,
        pagePositions: pageArticleDiapoPositions
    });

    function wadsInitDiapoPage(diapo_idx) {
        if (!wadsDiapoIsloaded) {
            if (jad.device.isMobile()) {
                var pageDiapoPositions = ["rectangle_atf"].concat(pageArticleDiapoPositions);
                elementsMapping = {
                    rectangle_atf: "rectangle_atf_diapo_mobile"
                };
            } else {
                var pageDiapoPositions = ["rectangle_atf", "rectangle_mtf"].concat(pageArticleDiapoPositions);
                elementsMapping = {
                    rectangle_atf: "rectangle_atf_diapo",
                    rectangle_mtf: "rectangle_mtf_diapo"
                };
            }
            jadConfigDiapo = $j.extend(true, {}, jadConfigDiapo, {
                elementsMapping: elementsMapping,
                pagePositions: pageDiapoPositions
            });

            jad.public.setConfig(jadConfigDiapo);
            jad.public.loadPositions();
            wadsNextAdIsloaded = true;

            if (!jad.device.isMobile()) {
                jad.public.displayPositions();
            }

            wdiapo.once('afterFullscreenLeave', function() {
                jad.public.setConfig(jadConfig);
                jad.public.loadPositions();
                jad.public.displayPositions();

                wadsDiapoIsloaded = false;
            });

            wadsDiapoIsloaded = true;
        }
    }

    function createNewPosition(element) {
         jad.cmd.push(function () {
            jad.public.createNewPosition(element);
            jad.public.refreshAds([element.name + '/' + element.divId]);
         });
    }

    WB = WB || {};
    WB.refreshAdMode = true;
    WB.ads_refresh = function(force, diapo_idx, is_ad){
        if (wadsDiapoIsloaded && diapo_idx && !jad.device.isMobile()) {
            jadConfigDiapo.page = "/120157152/PUREMEDIAS_FR_WEB/diaporama";
        }

        if (!jad.device.isMobile()) {
            jad.public.refreshAds();
        }

        // this is interdiapo
        if (is_ad) {
            $j('.wdiapo-big__swiper-prev.js-swiper-prev').hide().delay(1000).fadeIn();
            $j('.wdiapo-big__swiper-next.js-swiper-next').hide().delay(1000).fadeIn();
            jad.public.displayPositions();
            jQuery.Tracking().targets.t0.send({
                "hitType":  "event",
                "category": "diapo",
                "action":   "interdiapo",
                "label":    "interdiapo"
            });
            wadsNextAdIsloaded = false;
        } else if(!wadsNextAdIsloaded) {
            // Loading ads for next interdiapo
            jad.public.setConfig(jadConfigDiapo);
            jad.public.loadPositions();
            wadsNextAdIsloaded = true;
        }
    };
</script>
